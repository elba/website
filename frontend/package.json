{
  "author": "Andrés Rodríguez",
  "name": "elba-package-repo-web-frontend",
  "version": "1.0.0",
  "description": "Web Frontend for Elba's package repository",
  "scripts": {
    "start": "yarn dev",
    "dev": "parcel src/index.html",
    "build": "parcel build src/index.html --out-dir ../public"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.2",
    "@types/react-router-dom": "^4.3.1",
    "@types/marked": "^0.6",
    "autoprefixer": "^9.5.0",
    "modules": "^0.4.0",
    "parcel-bundler": "^1.12.2",
    "parcel-plugin-prerender": "^1.1.0",
    "postcss-modules": "^1.4.1",
    "sass": "^1.17.3",
    "typescript": "^3.3.3333",
    "typescript-plugin-css-modules": "^1.0.5"
  },
  "dependencies": {
    "marked": "^0.6",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-router-dom": "^5.0.0"
  },
  "postcss": {
    "modules": true,
    "plugins": {
      "autoprefixer": {
        "grid": true
      },
      "postcss-modules": {
        "camelCase": false
      }
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": false
  },
  "prerender": {
    "routes": [
      "/"
    ]
  }
}
